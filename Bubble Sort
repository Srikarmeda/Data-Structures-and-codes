#include<stdio.h>
#define MAX 100
void bubbleSort(int arr[],int len)
{
    for(int i=0;i<len-1;i++)
    {
        for(int j=0;j<len-i-1;j++)
        {
            if(arr[j]>arr[j+1])
            {
                int temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
}
int main(){
    int arr[]={6,8,5,10,7,12};
    int length=sizeof(arr)/sizeof(arr[0]);
    bubbleSort(arr,length);
    for(int i=0;i<length;i++)
    {
        printf("%d ",arr[i]);
    }
    return 0;
}
