#include<stdio.h>
#define MAX 100
void binary(int arr[],int l,int r,int n,int k)
{
    int m=(l+r)/2;
    if(k==arr[m])
    {
        printf("Element found at %d position",m+1);
    }
    else if(k<arr[m])
    {
        binary(arr,l,m-1,n,k);
    }
    else{
        binary(arr,m+1,n-1,n,k);
    }
}
int main(){
    int arr[]={1,4,6,9,16,25,36,49};
    int length=sizeof(arr)/sizeof(arr[0]);
    int key;
    scanf("%d",&key);
    int l=0;
    int r=length-1;
    binary(arr,l,r,length,key);
    return 0;
}
