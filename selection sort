#include<stdio.h>
#define MAX 100
void selectionSort(int arr[],int len)
{
    for(int i=0;i<len-1;i++)
    {
        int min=arr[i];
        for(int j=i+1;j<len;j++)
        {
            if(arr[j]<min)
            {
                min=arr[j];
                arr[j]=arr[i];
                arr[i]=min;
            }
        }
    }
}
int main(){
    int arr[]={23,16,30,4,41,27,26,47,11,46};
    int length=sizeof(arr)/sizeof(arr[0]);
    selectionSort(arr,length);
    for(int i=0;i<length;i++)
    {
        printf("%d ",arr[i]);
    }
    return 0;
}
