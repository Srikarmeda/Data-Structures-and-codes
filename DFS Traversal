#include <stdio.h>
#include<stdbool.h>
#define MAX_VERTICES 20
int graph[MAX_VERTICES][MAX_VERTICES];
bool visited[MAX_VERTICES];
int numVertices;
void dfs(int vertex)
{
    printf("%d ",vertex);
    visited[vertex]=true;
    for(int i=0;i<numVertices;i++)
    {
        if(graph[vertex][i]==1&&!visited[i])
        {
            dfs(i);
        }
    }
}
int main() {
    scanf("%d",&numVertices);
    for(int i=0;i<numVertices;i++)
    {
        visited[i]=false;
        for(int j=0;j<numVertices;j++)
        {
            graph[i][j]=0;
        }
    }
    for(int i=0;i<numVertices;i++)
    {
        for(int j=0;j<numVertices;j++)
        {
            scanf("%d",&graph[i][j]);
        }
    }
    int startVertex;
    scanf("%d",&startVertex);
    printf("DFS Traversal Order: ");
    dfs(startVertex);
    return 0;
}
